<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
          http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd
">    
    
    
    <bean id="name" class="java.lang.String" >
        <constructor-arg type="String" value="name-value"/>
    </bean>
    <bean id="secondName" class="java.lang.String">
        <constructor-arg type="String" value="secondName-value"/>
    </bean>
    <bean id="age" class="java.lang.Integer">
        <constructor-arg index="0" value="10"/>
    </bean>
    
    <bean id="myTarget" class="com.mycompany.my.spring5.core.autowired.xml.MyTarget" autowire="byName"/>
    
    
</beans>
