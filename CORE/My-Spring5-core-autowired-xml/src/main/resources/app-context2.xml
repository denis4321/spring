<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
          http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd
">    
    
  
    
    <bean id="myService" class="com.mycompany.my.spring5.core.autowired.xml2.MyService">
        <property name="name" value="serviceA"/>
    </bean>
    
    <bean id="myServiceObject" class="com.mycompany.my.spring5.core.autowired.xml2.MyService">
        <property name="name" value="serviceB"/>
    </bean>
        
    <bean id="myRepository" class="com.mycompany.my.spring5.core.autowired.xml2.MyRepository" autowire="byName"/>
    
    <bean id="myRepository2" class="com.mycompany.my.spring5.core.autowired.xml2.MyRepository" autowire="byName"/>
    
    
</beans>
