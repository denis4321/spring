<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
          http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd
">    
    <bean id="personA" class="com.mycompany.my.spring5.core.inject.collections.MyPerson">
        <property name="name" value="PersonA" />
        <property name="age" value="1" />
    </bean>
    <bean id="personB" class="com.mycompany.my.spring5.core.inject.collections.MyPerson">
        <property name="name" value="PersonB" />
        <property name="age" value="2" />
    </bean>
    <bean id="personC" class="com.mycompany.my.spring5.core.inject.collections.MyPerson">
        <property name="name" value="PersonC" />
        <property name="age" value="3" />
    </bean>
    
</beans>
