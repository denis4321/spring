<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
          http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd
">    
    <bean id="occupationA" class="com.mycompany.my.spring5.core.inject.collections.MyValue">
        <property name="occupation" value="occupationA" />
    </bean>
    <bean id="occupationB" class="com.mycompany.my.spring5.core.inject.collections.MyValue">
        <property name="occupation" value="occupationB" />        
    </bean>
    <bean id="occupationC" class="com.mycompany.my.spring5.core.inject.collections.MyValue">
        <property name="occupation" value="occupationC" />        
    </bean>
    
</beans>
